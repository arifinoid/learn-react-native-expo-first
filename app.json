{
  "expo": {
    "name": "learn-react-native-expo",
    "slug": "learn-react-native-expo",
    "description": "learn react native using expo",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arifinoid.rnexpo"
    },
    "android": {
      "package": "com.arifinoid.rnexpo"
    }
  }
}
